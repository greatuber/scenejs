{
    "plugins": [],
    "recurseDepth": 10,
    "opts": {
        "template": "./node_modules/daybrush-jsdoc-template",
        "destination": "./doc/"
    },
    "source": {
        "include": [
            "./packages/scenejs/src/",
            "./packages/scenejs/src/reactive",
            "./packages/react-scenejs/src/react-scenejs/",
            "./packages/vue-scenejs/src/",
            "./packages/vue2-scenejs/src/",
            "./packages/svelte-scenejs/src/svelte-scenejs/",
            "./README.md",
            "./node_modules/@scena/event-emitter/src/"
        ],
        "includePattern": "(.+\\.js(doc|x)?|.+\\.ts(doc|x)?)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": [
            "jsdoc",
            "closure"
        ]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "staticFiles": {
                "include": [
                    "./static"
                ]
            }
        }
    },
    "linkMap": {
        "IObject": "http://daybrush.com/utils/release/latest/doc/global.html#ObjectInterface"
    },
    "docdash": {
        "menu": {
            "Github repo": {
                "href": "https://github.com/daybrush/scenejs",
                "target": "_blank",
                "class": "menu-item",
                "id": "repository"
            },
            "Features": {
                "href": "https://daybrush.com/scenejs/features.html",
                "target": "_blank",
                "class": "menu-item",
                "id": "features"
            }
        }
    }
}