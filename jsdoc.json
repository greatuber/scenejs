{
    "plugins": [],
	"recurseDepth": 10,
	"opts": {
		"template": "./node_modules/daybrush-jsdoc-template",
		"destination": "./doc/"
    },
    "source": {
		"include": ["./src", "README.md"], 
        "includePattern": "(.+\\.js(doc|x)?|.+\\.ts(doc|x)?)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "linkMap": {
        "ObjectInterface": "http://daybrush.com/utils/release/latest/doc/global.html#ObjectInterface",
        "EventInterface": "http://daybrush.com/fjx/release/latest/doc/global.html#EventInterface",
        "CallbackType": "http://daybrush.com/fjx/release/latest/doc/global.html#CallbackType",
        "EventParamterType": "http://daybrush.com/fjx/release/latest/doc/global.html#EventParamterType",
        "ForEachInterface": "http://daybrush.com/fjx/release/latest/doc/global.html#ForEachInterface"
    }
}