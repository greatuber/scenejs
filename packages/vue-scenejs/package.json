{
    "name": "vue-scenejs",
    "version": "2.0.0-beta.0",
    "lernaHelperReleaseType": "prerelease",
    "lernaHelperPublishTag": "latest",
    "description": "A Vue 3 Component that create JavaScript & CSS timeline-based animation with Scene.js.",
    "main": "./dist/scene.cjs.js",
    "module": "./dist/scene.esm.js",
    "sideEffects": false,
    "types": "declaration/index.d.ts",
    "keywords": [
        "vue",
        "vue3",
        "composition",
        "reactive",
        "composition",
        "use",
        "scene",
        "scene.js",
        "scenejs",
        "keyframes",
        "timeline",
        "animate",
        "animation",
        "css",
        "requestAnimationFrame",
        "motion"
    ],
    
    "scripts": {
        "serve": "vue-cli-service serve demo",
        "build": "rm -rf dist && rollup -c && npm run declaration",
        "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
        "lint": "vue-cli-service lint --no-fix"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/daybrush/scenejs/blob/master/packages/vue3-scenejs"
    },
    "author": "Daybrush",
    "bugs": {
        "url": "https://github.com/daybrush/scenejs/issues"
    },
    "homepage": "https://github.com/daybrush/scenejs",
    "files": [
        "./*",
        "dist/*"
    ],
    "dependencies": {
        "@cfcs/vue3": "^0.0.10",
        "scenejs": "~1.8.3"
    },
    "devDependencies": {
        "@daybrush/builder": "^0.2.0",
        "@vue/cli-plugin-typescript": "~4.5.0",
        "@vue/cli-service": "~4.5.0",
        "@vue/compiler-sfc": "^3.0.0",
        "rollup-plugin-vue": "^6.0.0",
        "tslib": "^2.3.1",
        "typescript": "^4.5.0",
        "vue": "^3.1.1"
    }
}
