{
    "name": "svelte-scenejs",
    "version": "1.0.0-beta.2",
    "type": "module",
    "lernaHelperReleaseType": "prerelease",
    "lernaHelperPublishTag": "latest",
    "description": "A React Component that create JavaScript & CSS timeline-based animation with Scene.js.",
    "svelte": "./dist/scene.esm.js",
    "main": "./dist/scene.cjs.js",
    "module": "./dist/scene.esm.js",
    "sideEffects": false,
    "types": "declaration/index.d.ts",
    "keywords": [
        "svelte",
        "reactive",
        "use",
        "scene",
        "scene.js",
        "scenejs",
        "keyframes",
        "timeline",
        "animate",
        "animation",
        "css",
        "requestAnimationFrame",
        "motion"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/daybrush/scenejs/blob/master/packages/svelte-scenejs"
    },
    "author": "Daybrush",
    "bugs": {
        "url": "https://github.com/daybrush/scenejs/issues"
    },
    "homepage": "https://github.com/daybrush/scenejs",
    "files": [
        "./*",
        "dist/*"
    ],
    "scripts": {
        "dev": "vite dev",
        "start": "sirv public --no-clear",
        "build": "rollup -c && npm run declaration",
        "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json"
    },
    "dependencies": {
        "@cfcs/svelte": "^0.0.12",
        "scenejs": "~1.8.4"
    },
    "devDependencies": {
        "@daybrush/builder": "^0.2.0",
        "@sveltejs/adapter-auto": "^1.0.0",
        "@sveltejs/kit": "^1.0.0",
        "svelte": "^3.54.0",
        "svelte-check": "^2.9.2",
        "tslib": "^2.4.1",
        "typescript": "^4.5.0 <4.6.0",
        "vite": "^4.0.0"
    }
}
