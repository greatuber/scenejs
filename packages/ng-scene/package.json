{
  "name": "ng-scene",
  "version": "0.2.1",
  "main": "dist/common/index.js",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "types": "declaration/index.d.ts",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "rm -rf dist/ declaration/ && npm run build:common && npm run build:esm && npm run replace",
    "build:common": "tsc -p tsconfig.build.json --outDir ./dist/common --module commonjs",
    "build:esm": "tsc -p tsconfig.build.json",
    "replace": "replace-in-file '/** @class */' '/*#__PURE__*/' dist/*/*.js",
    "build2": "ng build",
    "lint": "ng lint"
  },
  "peerDependencies": {
    "@angular/core": "^5.0.0 || ^6.0.0",
    "@angular/common": "^5.0.0 || ^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daybrush/scenejs/tree/master/packages/ng-scene"
  },
  "keywords": [
    "scene",
    "scenejs",
    "animate",
    "animation",
    "css",
    "requestAnimationFrame",
    "motion",
    "angular",
    "angular 6",
    "angular 5",
    "ng"
  ],
  "author": "Daybrush",
  "license": "MIT",
  "dependencies": {
    "scenejs": "^1.0.0-beta8"
  },
  "devDependencies": {
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "@babel/preset-typescript": "^7.0.0",
    "core-js": "^2.5.4",
    "zone.js": "~0.8.26",
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-typescript": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "replace-in-file": "^3.4.2",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
