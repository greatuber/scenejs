{
	"compilerOptions": {
	  "outDir": "./outjs/",
	  "esModuleInterop": false,
		"sourceMap": true,
		"removeComments": true,
	  "noImplicitAny": true,
	  "module": "esnext",
		"target": "es6",
		"noUnusedLocals": true,
	  "skipLibCheck": true,
		"lib": ["es2015", "dom"],
	},
	"include": ["./src/**/*.ts"],
	"exclude": ["node_modules", "./src/index.umd.ts"]
  }